    <div>

      <pre class="base00-background-{{scheme-slug}} base05-{{scheme-slug}}">
<span class="base0E-{{scheme-slug}}">require</span> <span class="base0B-{{scheme-slug}}">"gem"</span>

<span class="base08-{{scheme-slug}}">string</span> = <span class="base0B-{{scheme-slug}}">"base16-{{scheme-slug}}"</span>
<span class="base08-{{scheme-slug}}">symbol</span> = <span class="base0B-{{scheme-slug}}">:base16-{{scheme-slug}}</span>
<span class="base08-{{scheme-slug}}">fixnum</span> = <span class="base09-{{scheme-slug}}">0</span>
<span class="base08-{{scheme-slug}}">float</span>  = <span class="base09-{{scheme-slug}}">0.00</span>
<span class="base08-{{scheme-slug}}">array</span>  = <span class="base0A-{{scheme-slug}}">Array</span>.<span class="base0D-{{scheme-slug}}">new</span>
<span class="base08-{{scheme-slug}}">array</span>  = [<span class="base0B-{{scheme-slug}}">'chris'</span>, <span class="base09-{{scheme-slug}}">85</span>]
<span class="base08-{{scheme-slug}}">hash</span>   = {<span class="base0B-{{scheme-slug}}">"test"</span> => <span class="base0B-{{scheme-slug}}">"test"</span>}
<span class="base08-{{scheme-slug}}">regexp</span> = <span class="base0C-{{scheme-slug}}">/[abc]/</span>

<span class="base03-{{scheme-slug}}"># This is a comment</span>
<span class="base0E-{{scheme-slug}}">class</span> <span class="base0A-{{scheme-slug}}">Person</span>
  
  <span class="base0D-{{scheme-slug}}">attr_accessor</span> <span class="base0B-{{scheme-slug}}">:name</span>
  
  <span class="base0E-{{scheme-slug}}">def</span> <span class="base0D-{{scheme-slug}}">initialize</span>(<span class="base08-{{scheme-slug}}">attributes</span> = {})
    <span class="base08-{{scheme-slug}}">@name</span> = <span class="base08-{{scheme-slug}}">attributes</span>[<span class="base0B-{{scheme-slug}}">:name</span>]
  <span class="base0E-{{scheme-slug}}">end</span>
  
  <span class="base0E-{{scheme-slug}}">def</span> <span class="base0E-{{scheme-slug}}">self</span>.<span class="base0D-{{scheme-slug}}">greet</span>
    <span class="base02-background-{{scheme-slug}}"><span class="base0B-{{scheme-slug}}">"hello"</span></span>
  <span class="base0E-{{scheme-slug}}">end</span>
<span class="base0E-{{scheme-slug}}">end</span>

<span class="base08-{{scheme-slug}}">person1</span> = <span class="base0A-{{scheme-slug}}">Person</span>.<span class="base0D-{{scheme-slug}}">new</span>(<span class="base0B-{{scheme-slug}}">:name</span> => <span class="base0B-{{scheme-slug}}">"Chris"</span>)
<span class="base0D-{{scheme-slug}}">print</span> <span class="base0A-{{scheme-slug}}">Person</span>::<span class="base0D-{{scheme-slug}}">greet</span>, <span class="base0B-{{scheme-slug}}">" "</span>, <span class="base08-{{scheme-slug}}">person1</span>.<span class="base0D-{{scheme-slug}}">name</span>, <span class="base0B-{{scheme-slug}}">"<span class="base09-{{scheme-slug}}">\n</span>"</span>
<span class="base0D-{{scheme-slug}}">puts</span> <span class="base0B-{{scheme-slug}}">"another </span><span class="base0F-{{scheme-slug}}">#{</span><span class="base0A-{{scheme-slug}}">Person</span>::<span class="base0D-{{scheme-slug}}">greet</span><span class="base0F-{{scheme-slug}}">}</span> <span class="base0F-{{scheme-slug}}">#{</span><span class="base08-{{scheme-slug}}">person1</span>.<span class="base0D-{{scheme-slug}}">name</span><span class="base0F-{{scheme-slug}}">}</span><span class="base0B-{{scheme-slug}}">"</span>
      </pre>

